PROGRAM EXCERCISE1;
VAR A : ARRAY(. 10 .) OF INTEGER;
        N : INTEGER;
PROCEDURE INPUT;
VAR I : INTEGER;
BEGIN
	N := READI;
	FOR I:=1 TO N DO
		A(.I.) := READI;
END;
PROCEDURE OUTPUT;
VAR I : INTEGER;
BEGIN
	FOR I := N TO 1 DO
		BEGIN
			CALL WRITEI(A(.I.));
			CALL WRITELN;
		END
END;
FUNCTION SUM : INTEGER;
VAR I: INTEGER;
        S: INTEGER;
BEGIN
	S := 0;
	I := 1;
	FOR I := 1 TO N DO
	BEGIN
		S := S + A(.I.);
	END
END;

BEGIN
	A %= 1;
	CALL INPUT;
	CALL OUTPUT;
	CALL WRITEI(SUM);
END.
